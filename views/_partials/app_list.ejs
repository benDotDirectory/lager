<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">App</th>
        </tr>
    </thead>
    <tbody>
        <% for (app in app_list) { %>
            <tr class="table-default">
                <td>
                    <%- partial("_partials/app.ejs", { app_name: app_list[app].app_name, api_key: app_list[app].api_key }) %>
                </td>
            </tr>
        <% } %>
    </tbody>
</table>